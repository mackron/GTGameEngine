#Sandbox
{
    width:            100%
    height:           100%
    flex-child-width: true
    child-plane:      horizontal
        
    visible: false
}
    
#SandboxLeftPanel
{
    width:  350px
    height: 100%
        
    allow-mouse-resize:true;
    right-gripper-width:8px;
        
    border-right:   1px #222
        
    min-width: 16px
}
    
#SandboxViewport
{
    width:  100%
    height: 100%
        
    min-width: 16px
}